{
  "name": "image-details-service",
  "version": "1.0.3",
  "private": true,
  "dependencies": {
    "aws-sdk": "^2.296.0",
    "date-fns": "^1.30.1",
    "dynamoose": "^1.2.0",
    "middy": "^0.19.2",
    "middy-middleware-lib": "^1.0.0",
    "object-mapper": "^5.0.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.11",
    "@types/jest": "^23.3.1",
    "@types/lambda-tester": "^3.5.1",
    "@types/node": "^10.9.4",
    "@types/request": "^2.47.1",
    "@types/uuid": "^3.4.4",
    "aws-types-lib": "^1.0.0",
    "axios": "^0.18.0",
    "coveralls": "^3.0.2",
    "jest": "^23.6.0",
    "jest-junit": "^6.0.0",
    "lambda-tester": "^3.5.0",
    "messages-lib": "^1.0.0",
    "prettier": "^1.15.3",
    "request": "^2.88.0",
    "ts-jest": "<23.10.0",
    "ts-loader": "^4.5.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.0.1",
    "uuid": "^3.3.2",
    "wait-for-expect": "^1.1.0",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-config": "^7.5.0",
    "zip-webpack-plugin": "^3.0.0"
  },
  "scripts": {
    "clean": "rimraf dist/*",
    "lint:fix": "tslint --fix -c tslint.json '{src,test}/**/*.ts'",
    "lint": "tslint -c tslint.json '{src,test}/**/*.ts'",
    "test:start-localdb": "docker-compose -f docker-compose.test.yml up -d",
    "test:stop-localdb": "docker-compose -f docker-compose.test.yml down",
    "test": "yarn test:start-localdb && jest src/ --runInBand; eval \"yarn test:stop-localdb && exit $?\"",
    "test:upload-coverage": "cat ./coverage/lcov.info | coveralls",
    "test:integration": "jest test/ --collectCoverage False",
    "build": "webpack --env.save_image_details_filename save-image-details.zip --env.save_image_store_filename save-image-url.zip"
  }
}
