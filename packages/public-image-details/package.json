{
  "name": "image-api",
  "version": "1.0.3",
  "private": true,
  "dependencies": {
    "aws-sdk": "^2.296.0",
    "image-lib": "^1.0.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.11",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.9.4",
    "@types/request": "^2.47.1",
    "axios": "^0.18.0",
    "jest": "^23.5.0",
    "request": "^2.88.0",
    "ts-jest": "^23.1.3",
    "ts-loader": "^4.5.0",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1",
    "webpack": "^4.16.5",
