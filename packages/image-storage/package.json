{
  "name": "image-storage",
  "version": "1.0.0",
  "private": true,
  "description": "Stores publicly accessible images and sends a command to create a daily image of them",
  "main": "handler.js",
  "scripts": {
    "build": "serverless package -v",
    "deploy:dev": "serverless deploy -v",
    "remove:dev": "yarn remove:dev-s3-objects && serverless deploy -v",
    "remove:dev-s3-objects": "aws s3 rm s3://draw-by-days-dev-images --recursive",
    "deploy:component": "serverless deploy -v --stage component",
    "remove:component": "serverless remove -v --stage component",
    "test:sqs:start": "docker-compose up -d",
    "test:sqs:stop": "docker-compose down",
    "test": "yarn test:sqs:start && jest __tests__/integration --runInBand; eval \"yarn test:sqs:stop && exit $?\"",
    "test:component": "yarn deploy:component && jest __tests__/component --runInBand; eval \"yarn remove:component && exit $?\""
  },
  "dependencies": {
    "@anttiviljami/serverless-stack-output": "^0.3.1",
    "@laconia/core": "^1.2.0",
    "aws-sdk": "^2.471.0",
    "aws-xray-sdk": "^2.3.3",
    "serverless": "^1.44.1",
    "serverless-pseudo-parameters": "^2.4.0",
    "serverless-stage-manager": "^1.0.5",
    "daily-image-api-command": "^1.0.0"
  },
  "devDependencies": {
    "@types/async-retry": "^1.4.1",
    "@types/aws-lambda": "^8.10.17",
    "@types/axios": "^0.14.0",
    "@types/jest": "=23.3.13",
    "@types/lambda-tester": "^3.5.1",
    "@types/node": "^8.0.57",
    "@types/uuid": "^3.4.4",
    "async-retry": "^1.2.3",
    "axios": "^0.19.0",
    "daily-image-api": "^1.0.0",
    "jest": "^24.8.0",
    "lambda-tester": "^3.5.0",
    "prettier": "^1.18.2",
    "serverless-webpack": "^5.3.0",
    "source-map-support": "^0.5.6",
    "ts-jest": "^24.0.2",
    "ts-loader": "^4.2.0",
    "typescript": "^2.9.2",
    "uuid": "^3.3.2",
    "webpack": "^4.5.0"
  }
}
