{
  "name": "image-storage",
  "version": "1.0.0",
  "private": true,
  "description": "Stores publicly accessible images and sends a command to create a daily image of them",
  "main": "handler.js",
  "scripts": {
    "build": "serverless package -v",
    "deploy": "serverless deploy -v",
    "remove:dev-s3-objects": "aws s3 rm s3://draw-by-days-dev-images --recursive",
    "remove:resources": "serverless remove -v",
    "remove": "yarn remove:dev-s3-objects && yarn remove:resources",
    "test": "jest __tests__/"
  },
  "dependencies": {
    "@laconia/core": "^1.0.0",
    "apollo-cache-inmemory": "^1.1.0",
    "apollo-client": "^2.0.3",
    "apollo-link": "^1.0.3",
    "apollo-link-http": "^1.2.0",
    "aws-appsync": "^1.4.0",
    "aws-sdk": "^2.441.0",
    "aws-xray-sdk": "^2.3.3",
    "graphql": "^14.1.1",
    "graphql-tag": "^2.9.2",
    "isomorphic-fetch": "^2.2.1",
    "node-fetch": "^2.3.0",
    "serverless": "^1.41.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.17",
    "@types/jest": "=23.3.13",
    "@types/node": "^8.0.57",
    "daily-image-api": "^1.0.0",
    "jest": "^24.7.1",
    "prettier": "^1.17.0",
    "serverless-webpack": "^5.1.1",
    "source-map-support": "^0.5.6",
    "ts-jest": "^24.0.2",
    "ts-loader": "^4.2.0",
    "typescript": "^2.9.2",
    "webpack": "^4.5.0"
  }
}
