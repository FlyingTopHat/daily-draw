{
  "name": "image-ingest",
  "version": "1.0.0",
  "private": true,
  "description": "Stores publicly accessible images and sends a command to create a daily image of them",
  "main": "handler.js",
  "scripts": {
    "build": "sls package -v",
    "test:s3:start": "docker-compose up -d",
    "test:s3:stop": "docker-compose down",
    "test": "yarn test:s3:start && jest __tests__/integration --runInBand; eval \"yarn test:s3:stop && exit $?\"",
    "test:component": "yarn deploy:component && jest __tests__/component --runInBand; eval \"yarn remove:component && exit $?\"",
    "deploy:prod": "sls deploy -v --stage prod",
    "deploy:dev": "sls deploy -v --stage dev",
    "remove:dev": "sls remove -v --stage dev",
    "deploy:ci": "sls deploy -v --stage ci",
    "remove:ci": "sls remove -v --stage ci",
    "deploy:component": "sls deploy -v --stage component",
    "remove:component": "sls remove -v --stage component"
  },
  "dependencies": {
    "@laconia/core": "^1.2.0",
    "aws-sdk": "^2.471.0",
    "aws-xray-sdk": "^2.3.3",
    "filenamify-url": "^2.1.0",
    "node-fetch": "^2.5.0",
    "pixabay-api": "^1.0.4",
    "serverless": "^1.44.1",
    "serverless-stage-manager": "^1.0.5",
    "@anttiviljami/serverless-stack-output": "^0.3.1"
  },
  "devDependencies": {
    "@types/async-retry": "^1.4.1",
    "@types/aws-lambda": "^8.10.17",
    "@types/filenamify-url": "^1.0.1",
    "@types/jest": "=23.3.13",
    "@types/lambda-tester": "^3.5.1",
    "@types/nock": "^10.0.3",
    "@types/node": "^8.0.57",
    "@types/node-fetch": "^2.3.3",
    "image-storage": "^1.0.0",
    "jest": "^24.8.0",
    "lambda-tester": "^3.5.0",
    "nock": "^10.0.6",
    "serverless-webpack": "^5.3.0",
    "source-map-support": "^0.5.6",
    "ts-jest": "^24.0.2",
    "ts-loader": "^4.2.0",
    "typescript": "^2.9.2",
    "webpack": "^4.5.0"
  }
}
