{
    "version": "2017-02-28",
    "operation": "Scan",
    "filter": {
        "expression": "#date = :date",
        "expressionNames": {
            "#date" : "date"
        },
        "expressionValues": {
            ":date": {
                "S": "$context.arguments.date"
            }
        }
    }
}
