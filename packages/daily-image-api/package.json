{
  "name": "daily-image-api",
  "version": "1.0.0",
  "private": true,
  "description": "Provides read/write of daily images",
  "scripts": {
    "build": "serverless package -v",
    "deploy:dev": "serverless deploy -v",
    "remove:dev": "serverless remove -v",
    "deploy:component": "serverless deploy -v --stage component",
    "remove:component": "serverless remove -v --stage component",
    "test:dynamodb:start": "docker-compose up -d",
    "test:dynamodb:stop": "docker-compose down",
    "test:dynamodb:deploy-table": "serverless dynamodb migrate",
    "test": "yarn test:dynamodb:start && yarn test:dynamodb:deploy-table && jest __tests__/integration --runInBand; eval \"yarn test:dynamodb:stop && exit $?\"",
    "test:component": "yarn deploy:component && jest __tests__/component --runInBand; eval \"yarn remove:component && exit $?\""

  },
  "dependencies": {
    "@laconia/adapter": "^1.1.0",
    "@laconia/core": "^1.2.0",
    "@laconia/event": "^1.1.0",
    "aws-sdk": "^2.471.0",
    "aws-xray-sdk": "^2.3.3",
    "serverless": "^1.44.1",
    "serverless-dynamodb-local": "^0.2.38",
    "serverless-stage-manager": "^1.0.5",
    "@anttiviljami/serverless-stack-output": "^0.3.1"
  },
  "devDependencies": {
    "@types/async-retry": "^1.4.1",
    "async-retry": "^1.2.3",
    "@types/aws-lambda": "^8.10.17",
    "@types/axios": "^0.14.0",
    "@types/node": "^8.0.57",
    "@types/uuid": "^3.4.4",
    "uuid": "^3.3.2",
    "axios": "^0.18.0",
    "prettier": "^1.18.2",
    "serverless-webpack": "^5.3.0",
    "source-map-support": "^0.5.6",
    "ts-loader": "^4.2.0",
    "typescript": "^2.9.2",
    "webpack": "^4.5.0"
  }
}
