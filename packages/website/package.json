{
  "name": "website",
  "version": "1.0.0",
  "private": true,
  "main": "src/index.tsx",
  "homepage": "http://draw-by-days-website.s3-website-us-east-1.amazonaws.com",
  "dependencies": {
    "@material-ui/core": "latest",
    "@types/date-fns": "^2.6.0",
    "@types/react": "*",
    "@types/react-dom": "*",
    "axios": "^0.18.0",
    "date-fns": "^1.29.0",
    "react": "latest",
    "react-dom": "latest",
    "react-scripts-ts": "latest"
  },
  "devDependencies": {
    "@types/jest": "*",
    "@types/node": "^10.11.2",
    "cypress": "^3.1.0",
    "jest-junit": "^6.0.0",
    "rimraf": "^2.6.2",
    "start-server-and-test": "^1.7.1",
    "typescript": "latest"
  },
  "scripts": {
    "clean": "rimraf build/*",
    "lint": "tslint -c tslint.json 'src/**/*.ts{,x}'",
    "start": "react-scripts-ts start",
    "test": "CI=true react-scripts-ts test",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:component": "start-server-and-test start http://localhost:3000 cy:run",
    "build": "react-scripts-ts build",
    "publish:s3": "WEBSITE_BUCKET_NAME=$(cd $npm_package_config_deployment_path && terraform output bucket_name) && aws s3 cp --recursive ./build s3://$WEBSITE_BUCKET_NAME/"
  }
}
