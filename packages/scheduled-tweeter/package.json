{
  "name": "scheduled-tweeter",
  "version": "1.0.0",
  "private": true,
  "description": "Tweets an image on a scheduled basis",
  "main": "handler.js",
  "scripts": {
    "lint": "tslint -c tslint.json '{__tests__}/**/*.ts'",
    "test:integration": "jest __tests__/integration/",
    "test": "jest __tests__/unit/",
    "deploy:dev": "sls deploy --stage dev",
    "destroy:dev": "sls remove --stage dev",
    "deploy:prod": "sls deploy --stage prod",
    "destroy:prod": "sls remove --stage prod"
  },
  "dependencies": {
    "aws-sdk": "^2.585.0",
    "getenv": "^1.0.0",
    "source-map-support": "^0.5.16",
    "tmp": "^0.1.0",
    "twit": "^2.2.11"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.37",
    "@types/getenv": "^1.0.0",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.14",
    "@types/tmp": "^0.1.0",
    "@types/twit": "^2.2.23",
    "@types/uuid": "^3.4.7",
    "fork-ts-checker-webpack-plugin": "^3.1.1",
    "jest": "^24.9.0",
    "pixabay-api": "^1.0.4",
    "serverless": "^1.59.2",
    "serverless-stage-manager": "^1.0.5",
    "serverless-webpack": "^5.3.1",
    "ts-jest": "^24.2.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.3",
    "uuid": "^7.0.0",
    "webpack": "^4.41.2",
    "webpack-node-externals": "^1.7.2"
  },
